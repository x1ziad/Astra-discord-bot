{
  "bot": {
    "name": "Astra",
    "version": "2.0.1",
    "description": "Advanced Discord bot with AI capabilities",
    "prefix": "!",
    "debug": false
  },
  "discord": {
    "token": "YOUR_DISCORD_BOT_TOKEN",
    "client_id": "YOUR_CLIENT_ID",
    "client_secret": null,
    "prefix": "!"
  },
  "openai": {
    "api_key": "YOUR_OPENAI_API_KEY",
    "model": "gpt-4",
    "max_tokens": 1000,
    "temperature": 0.7
  },
  "nasa": {
    "api_key": "YOUR_NASA_API_KEY"
  },
  "database": {
    "url": "sqlite:///data/astra.db"
  },
  "features": {
    "ai_chat": true,
    "space_content": true,
    "quiz_system": true,
    "analytics": true,
    "admin_commands": true,
    "auto_moderation": false,
    "voice_support": false
  },
  "colors": {
    "primary": "#7289da",
    "success": "#43b581",
    "warning": "#faa61a",
    "error": "#f04747",
    "info": "#17a2b8"
  },
  "limits": {
    "max_concurrent_requests": 10,
    "request_timeout": 30,
    "cache_ttl": 3600
  }
}