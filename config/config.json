{
  "bot": {
    "token": "${DISCORD_TOKEN}",
    "client_id": "${DISCORD_CLIENT_ID}",
    "owner_id": "${OWNER_ID}",
    "name": "Astra",
    "version": "2.0.0",
    "description": "Advanced AI-powered Discord bot",
    "prefix": "!",
    "status": "online",
    "activity_type": "watching",
    "activity_name": "the cosmos",
    "command_sync_on_ready": true,
    "command_sync_on_join": false,
    "cleanup_on_leave": true,
    "features": {
      "enable_ai": true,
      "enable_voice": false,
      "enable_moderation": true,
      "enable_analytics": true,
      "enable_auto_responses": true,
      "enable_slash_commands": true,
      "space_content": true,
      "space_content.iss_tracking": true,
      "space_content.launch_notifications": true,
      "notion_integration": true,
      "notion_tasks": true,
      "notion_reminders": true,
      "notion_notes": true
    }
  },
  "ai": {
    "api_key": "${AI_API_KEY}",
    "base_url": "${AI_BASE_URL}",
    "model": "${AI_MODEL}",
    "max_tokens": "${AI_MAX_TOKENS}",
    "temperature": 0.7,
    "provider_name": "${AI_PROVIDER}"
  },
  "database": {
    "path": "data/astra.db",
    "backup_enabled": true,
    "backup_interval": 3600,
    "connection_pool_size": 10
  },
  "cache": {
    "enabled": true,
    "default_ttl": 300,
    "max_size": 10000,
    "redis_enabled": false,
    "redis_url": null
  },
  "api_keys": {
    "openrouter": "${OPENROUTER_API_KEY}",
    "nasa": "${NASA_API_KEY}",
    "notion": "${NOTION_TOKEN}",
    "magichour": "${MAGICHOUR_API_KEY}"
  },
  "channels": {
    "space_channel_id": "${SPACE_CHANNEL_ID}"
  },
  "logging": {
    "level": "INFO",
    "file_enabled": true,
    "file_path": "logs/astra.log",
    "console_enabled": true,
    "max_file_size": 10485760,
    "backup_count": 5
  },
  "guilds": {},
  "updated_at": "2025-09-21T00:00:00Z"
}