{
  "bot_settings": {
    "name": "AstraBot",
    "version": "2.0.0",
    "description": "Advanced Discord bot with AI capabilities",
    "author": "AstraBot Development Team",
    "prefix": "!",
    "status": "online",
    "activity": "Watching the cosmos",
    "max_guilds": 1000,
    "enable_slash_commands": true,
    "enable_context_menus": true
  },
  "ai_settings": {
    "default_provider": "consolidated",
    "enable_universal_responses": true,
    "conversation_memory": true,
    "max_conversation_length": 20,
    "response_delay_min": 1.0,
    "response_delay_max": 3.0,
    "enable_emotional_analysis": true,
    "enable_personality_adaptation": true,
    "providers": {
      "openai": {
        "enabled": true,
        "model": "gpt-4",
        "max_tokens": 1000,
        "temperature": 0.7
      },
      "openrouter": {
        "enabled": true,
        "model": "anthropic/claude-3-sonnet",
        "max_tokens": 1000,
        "temperature": 0.7
      },
      "anthropic": {
        "enabled": true,
        "model": "claude-3-sonnet-20240229",
        "max_tokens": 1000,
        "temperature": 0.7
      }
    }
  },
  "discord": {
    "token": "YOUR_DISCORD_BOT_TOKEN_HERE",
    "prefix": "!",
    "intents": {
      "messages": true,
      "guilds": true,
      "members": true,
      "reactions": true
    }
  },
  "database": {
    "path": "data/astra.db",
    "backup_interval": 3600,
    "connection_pool_size": 15,
    "enable_wal_mode": true
  },
  "openai": {
    "api_key": "YOUR_OPENAI_API_KEY_HERE",
    "model": "gpt-4",
    "max_tokens": 1000,
    "temperature": 0.7
  },
  "ai": {
    "openai": {
      "api_key": "YOUR_OPENAI_API_KEY_HERE",
      "model": "gpt-4",
      "max_tokens": 1000,
      "temperature": 0.7
    },
    "azure": {
      "endpoint": "YOUR_AZURE_ENDPOINT_HERE",
      "api_key": "YOUR_AZURE_API_KEY_HERE",
      "speech_key": "YOUR_AZURE_SPEECH_KEY_HERE",
      "speech_region": "westus"
    }
  },
  "features": {
    "enable_ai": true,
    "enable_voice": false,
    "enable_moderation": true,
    "enable_analytics": true
  },
  "performance": {
    "max_concurrent_tasks": 50,
    "cache_ttl": 300,
    "rate_limit_per_minute": 60,
    "memory_warning_threshold": 500,
    "memory_critical_threshold": 800
  },
  "logging": {
    "level": "INFO",
    "max_file_size": "10MB",
    "backup_count": 5,
    "console_output": true
  }
}
